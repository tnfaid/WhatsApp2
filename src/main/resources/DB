CREATE TABLE `WHATSAPP_NUMBER` (
  `WHATSAPP_ID` int(10) NOT NULL AUTO_INCREMENT,
  `PHONE` varchar(15) CHARACTER SET latin1 NOT NULL,
  `UNIQUE_ID` varchar(15) CHARACTER SET latin1 NOT NULL,
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'number added date timestamp',
  `MESSAGE_TYPE` varchar(45) DEFAULT NULL,
  `WEBHOOK_STATUS` varchar(10) DEFAULT NULL,
  `DATA` varchar(100) DEFAULT NULL,
  `SiZE` int(10) DEFAULT NULL,
  PRIMARY KEY (`WHATSAPP_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4;
